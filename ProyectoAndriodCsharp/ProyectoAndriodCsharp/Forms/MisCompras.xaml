<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProyectoAndriodCsharp.Forms.MisCompras">
    <ContentPage.Content>
        <Grid>
            <BoxView Color="Black"/>
        <StackLayout>
                <Label Text="Mis compras" FontSize="30" TextColor="White"/>
                <Button Text="Volver" Clicked="Button_Volver" BackgroundColor="#ecc42c"/>
            <ScrollView>
                    <Label x:Name="label_compras" TextColor="White"/>
            </ScrollView>
        </StackLayout>
        </Grid>
    </ContentPage.Content>

</ContentPage>