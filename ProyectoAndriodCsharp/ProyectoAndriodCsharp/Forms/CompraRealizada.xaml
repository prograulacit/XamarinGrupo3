<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProyectoAndriodCsharp.Forms.CompraRealizada">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Compra realizada con exito" FontSize="30"/>
            <Label Text="Para ver las compras realizadas dirijase a la ventana de compras realizadas."/>
            <Label Text="Para administrar sus abonos dirijase a la ventana de abonos."/>

            <Button Text="Ir a Mis compras" Clicked="Button_MisCompras"/>
            <Button Text="Volver a productos" Clicked="Button_Volver" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>